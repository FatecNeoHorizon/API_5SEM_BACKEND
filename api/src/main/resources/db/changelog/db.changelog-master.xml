<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
           http://www.liquibase.org/xml/ns/dbchangelog
           http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <!--
      Master changelog. Put your changelog files under
      src/main/resources/db/changelog/changelogs/
      and they will be picked up automatically by Liquibase.
    -->

    <includeAll path="db/changelog/changelogs" relativeToChangelogFile="false"/>

</databaseChangeLog>
